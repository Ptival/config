! The next four commands make Ctrl+Arrow send the same keycode as xterm would
! Useful for vim not to get confused by urxvt...
URxvt.keysym.Control-Up:    \033[1;5A
URxvt.keysym.Control-Down:  \033[1;5B
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

!urxvt*foreground: #f2f2f2
urxvt*background: #202020

!urxvt*color0:   #6c6c6c
!urxvt*color1:   #e9897c
!urxvt*color2:   #b6e77d
!urxvt*color3:   #eedd44
urxvt*color4:   #99adeb
!urxvt*color5:   #ea96eb
!urxvt*color6:   #c9caec
!urxvt*color7:   #f2f2f2
!urxvt*color8:   #747474
!urxvt*color9:   #f99286
!urxvt*color10:  #c3f786
!urxvt*color11:  #ffee55
urxvt*color12:  #96cefb
!urxvt*color13:  #fba1fb
!urxvt*color14:  #d7d9fc
!urxvt*color15:  #e2e2e2

!URxvt*scrollBar_right:  false
URxvt*scrollBar:        false
